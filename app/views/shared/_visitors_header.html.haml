    #navbarSupportedContent.collapse.navbar-collapse
      %ul.navbar-nav.ml-auto.scroll-to
        %li.nav-item
          %a.nav-link.active{"data-scroll" => "data-scroll", :href => "#home"} Home
        %li.dropdown.nav-item
          %span.dropdown-toggle.menu-drop-toggle.nav-link{"aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown", :role => "button"} Services
          %ul.dropdown-menu.dropdown-menu-right
            %li
              %a.dropdown-item{"data-scroll" => "data-scroll", :href => "#agency"} About Us
