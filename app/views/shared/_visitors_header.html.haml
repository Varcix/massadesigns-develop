              %a.dropdown-item{:href => '#services'} Graphic Design
            %li
              %a.dropdown-item{:href => '#virtual-staging'} 3D Virtual Staging
            %li
              %a.dropdown-item{:href => '#services'} Aerial Photography
            %li
              %a.dropdown-item{:href => '#services'} Video Walk-through
        %li.nav-item
          %a.nav-link{"data-scroll" => "data-scroll", :href => "#contact", :style => 'color:#fff;'} Book an Appointment
        %li.nav-item
          %a.nav-link{"data-scroll" => "data-scroll", :href => "#work"} Portfolio
        %li.nav-item
